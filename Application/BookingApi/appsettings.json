{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "ScopeProvider": "Debug",
      "MyDebugMiddleware.Request": "Warning",
      "MyDebugMiddleware.Response": "Trace",
      "MessageQueue.Subscribe": "Warning",
      "MessageQueue.Notify": "Trace",
      "Microsoft.AspNetCore": "Debug",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database": "Debug",
      "Microsoft.EntityFrameworkCore.DbContext": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": false,
        "TimestampFormat": "\nHH:mm:ss ",
        "UseUtcTimestamp": true,
        "IncludeScopes": false
      }
    }
  },
  "AllowedHosts": "*",
}
