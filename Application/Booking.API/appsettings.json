{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database": "Debug"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": false,
        "TimestampFormat": "\nHH:mm:ss ",
        "UseUtcTimestamp": true,
        "IncludeScopes": false
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Debug": "Data Source=file:($Context)?mode=memory&cache=shared;",
    "Release": "Data Source=./($Context).db;"
  }
}
