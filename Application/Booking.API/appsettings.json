{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss "
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Debug": "Data Source=file:($Context)?mode=memory&cache=shared;",
    "Release": "Data Source=./($Context).db;"
  }
}
